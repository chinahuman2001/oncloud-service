{
  "requires": {
    "auth": {
      "strategy": "JWT"
    },
    "db": {
      "kind": "hana",
      "multiTenant": true,
      "vcap": {
        "label": "service-manager"
      }
    },
    "uaa": {
      "kind": "xsuaa"
    }
  },
  "hana": {
    "deploy-format": "hdbcds"
  },
  "mtx": {
    "element-prefix": "Z_",
    "namespace-blacklist": ["com.sap.", "sap."],
    "entity-whitelist": [],
    "service-whitelist": []
  },
  "auth": {
    "passport": {
      "strategy": "JWT"
    }
  }
}
